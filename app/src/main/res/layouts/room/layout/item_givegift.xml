<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingStart="@dimen/dp_16"
    android:paddingEnd="@dimen/dp_20"
    android:layout_marginBottom="@dimen/dp_20">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true">
        <TextView
            android:id="@+id/txt_give_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#9FD36C"
            android:textSize="@dimen/sp_15"/>
        <TextView
            android:id="@+id/txt_give_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@id/txt_give_name"
            android:textColor="#ffffff"
            android:textSize="@dimen/sp_15" />
        <TextView
            android:id="@+id/txt_give_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/txt_give_name"
            android:layout_marginTop="@dimen/dp_8"
            android:textColor="#ffffff"
            android:textSize="@dimen/sp_13" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_alignParentEnd="true"
        android:gravity="center_vertical"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/dp_5"
            android:textColor="#ff0000"
            android:textSize="@dimen/sp_14"
            android:text="x1"/>
        <com.facebook.drawee.view.SimpleDraweeView
            android:id="@+id/iv_give_gift"
            android:layout_width="@dimen/dp_34"
            android:layout_height="@dimen/dp_34" />
    </LinearLayout>

